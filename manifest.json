{
   "background": {
      "scripts": [ "background.js" ]
   },

   "browser_action": {
      "default_icon": "thermoscan-128.png"
   },
   "description": "Send current PMC page to ThermoScan server.",
   "icons": {
      "256": "thermoscan-256.png",
      "128": "thermoscan-128.png",
      "48" : "thermoscan-48.png"
   },
   "manifest_version": 2,
   "name": "ThermoScan",
   "permissions": [ "activeTab", "downloads", "tabs", "https://folding.biofold.org/*" ],
   "version": "0.0.0.1"
}
